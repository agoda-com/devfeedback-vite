{
    "name": "agoda-devfeedback-vite",
    "version": "0.1.3",
    "description": "Vite plugin for collecting and reporting development feedback metrics",
    "type": "module",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
      "dist",
      "README.md"
    ],
    "scripts": {
      "build": "tsup",
      "prepublishOnly": "npm run build",
      "test": "vitest",
      "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.ts\""
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/agoda-com/devfeedback-vite.git"
    },
    "keywords": [
      "vite",
      "vite-plugin",
      "metrics",
      "development",
      "hmr",
      "performance"
    ],
    "author": "Agoda",
    "license": "Apache-2.0",
    "bugs": {
      "url": "https://github.com/agoda-com/devfeedback-vite/issues"
    },
    "homepage": "https://github.com/agoda-com/devfeedback-vite#readme",
    "peerDependencies": {
      "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
    },
    "dependencies": {
      "uuid": "^9.0.0",
      "node-fetch": "^3.3.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/uuid": "^9.0.0",
        "@eslint/js": "^8.57.0",
        "typescript-eslint": "^7.1.0",
        "eslint": "^8.57.0",
        "vitest": "^1.0.0",
        "prettier": "^3.0.0",
        "typescript": "^5.3.0",
        "@types/jest": "^29.0.0",
        "tsup": "^8.0.0",
        "eslint-config-prettier": "^9.0.0"
      }
  }